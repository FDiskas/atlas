{
  "name": "@abstractflo/atlas-server",
  "version": "3.1.0",
  "description": "> TODO: description",
  "author": "Florian Uhlrich <f.uhlrich@abstractmedia.de>",
  "homepage": "",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.esm.js"
  },
  "scripts": {
    "prebuild": "yarn clean && yarn types",
    "postbuild": "yarn local:pack",
    "prewatch": "yarn clean",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "clean": "rimraf {dist,build,types,*.d.ts,*.tgz}",
    "types": "dts-bundle-generator -o dist/index.d.ts src/index.ts --inline-declare-externals --sort --no-check",
    "local:pack": "yarn pack --filename server.tgz"
  },
  "peerDependencies": {
    "@abstractflo/atlas-shared": "^3.1.0",
    "@altv/types-server": "^1.9.5"
  },
  "devDependencies": {
    "@abstractflo/atlas-shared": "*",
    "@altv/types-server": "*"
  }
}
