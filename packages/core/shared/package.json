{
  "name": "@abstractflo/atlas-shared",
  "version": "3.1.0",
  "description": "> TODO: description",
  "author": "Florian Uhlrich <f.uhlrich@abstractmedia.de>",
  "homepage": "",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.esm.js"
  },
  "scripts": {
    "prebuild": "yarn clean && yarn types",
    "postbuild": "yarn local:pack",
    "prewatch": "yarn clean",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "clean": "rimraf {dist,build,types,*.d.ts,*.tgz}",
    "types": "dts-bundle-generator --config dts-bundle.json",
    "local:pack": "yarn pack --filename shared.tgz"
  },
  "dependencies": {
    "@abraham/reflection": "^0.8.0",
    "rxjs": "^7.2.0",
    "tsyringe": "^4.6.0"
  },
  "peerDependencies": {
    "@altv/types-shared": "^1.0.1"
  },
  "devDependencies": {
    "@altv/types-shared": "*"
  }
}
