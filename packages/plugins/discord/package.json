  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.esm.js"
  },
  "scripts": {
    "prebuild": "yarn clean && yarn types",
    "postbuild": "yarn local:pack",
