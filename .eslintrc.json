{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["prettier"],
  "extends": [
    "prettier"
  ],
  "parserOptions": {
    "extraFileExtensions": [
      ".cjs"
    ],
    "project": "./tsconfig.eslint.json",
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
